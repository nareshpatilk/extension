{
    "manifest_version": 3,
    "name": "Chart Generate Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "chart_generator.html",
      "default_icon": "hello_extensions.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["_favicon/*"],
        "matches": ["<all_urls>"]
      }
    ],
    "permissions": [
      "tabs",
      "scripting",
      "activeTab"
    ]
    
  }

  